{
  "version": "0.2.0",
  "configurations": [
    
    {
      "name": "Run stock_lens.py",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}\\stock_lens.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Module: indicators example",
      "type": "python",
      "request": "launch",
      "module": "src.features.run_indicators_example",
      "args": ["--symbol", "BTCUSDT", "--interval", "1d"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Module: market_data (binance)",
      "type": "python",
      "request": "launch",
      "module": "src.data_ingestion.market_data",
      "args": ["--symbol", "BTCUSDT", "--source", "binance", "--interval", "1d", "--limit", "1000"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
